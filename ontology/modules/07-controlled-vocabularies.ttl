@prefix salmon: <http://w3id.org/salmon/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://w3id.org/salmon-domain-ontology/modules/07-controlled-vocabularies> a owl:Ontology ;
  rdfs:label "Salmon Domain Ontology Module 07: Controlled vocabularies"@en ;
  rdfs:comment "Cross-agency concept schemes and values for salmon measurement context, escapement methods, benchmark qualifiers, and status vocabularies."@en .

#################################################################
# Concept schemes
#################################################################

salmon:MeasurementContextScheme a skos:ConceptScheme ;
  skos:prefLabel "Measurement context scheme"@en ;
  skos:hasTopConcept salmon:MeasurementContext .

salmon:LifePhaseScheme a skos:ConceptScheme ;
  skos:prefLabel "Life phase scheme"@en ;
  skos:hasTopConcept salmon:LifePhase .

salmon:SalmonOriginScheme a skos:ConceptScheme ;
  skos:prefLabel "Salmon origin scheme"@en ;
  skos:hasTopConcept salmon:SalmonOrigin .

salmon:BenchmarkLevelScheme a skos:ConceptScheme ;
  skos:prefLabel "Benchmark level scheme"@en .

salmon:WSPBiologicalStatusZoneScheme a skos:ConceptScheme ;
  skos:prefLabel "Biological status zone scheme"@en ;
  skos:hasTopConcept salmon:WSPBiologicalStatusZone .

salmon:EnumerationMethodScheme a skos:ConceptScheme ;
  skos:prefLabel "Escapement enumeration method scheme"@en ;
  skos:hasTopConcept salmon:EnumerationMethod .

salmon:EstimateMethodScheme a skos:ConceptScheme ;
  skos:prefLabel "Escapement estimate method scheme"@en ;
  skos:hasTopConcept salmon:EstimateMethod .

salmon:EstimateTypeScheme a skos:ConceptScheme ;
  skos:prefLabel "Escapement estimate type scheme"@en ;
  skos:hasTopConcept salmon:EstimateType .

salmon:DowngradeCriteriaScheme a skos:ConceptScheme ;
  skos:prefLabel "Downgrade criteria scheme"@en ;
  skos:hasTopConcept salmon:DowngradeCriteria .

#################################################################
# Context and constraints
#################################################################

salmon:MeasurementContext a skos:Concept ;
  skos:prefLabel "Measurement context"@en ;
  skos:inScheme salmon:MeasurementContextScheme ;
  skos:definition "A context qualifier used to disambiguate interpretation of a measurement."@en .

salmon:CatchContext a skos:Concept ;
  skos:prefLabel "Catch context"@en ;
  skos:broader salmon:MeasurementContext ;
  skos:inScheme salmon:MeasurementContextScheme .

salmon:RunContext a skos:Concept ;
  skos:prefLabel "Run context"@en ;
  skos:broader salmon:MeasurementContext ;
  skos:inScheme salmon:MeasurementContextScheme .

salmon:SpawnerStageContext a skos:Concept ;
  skos:prefLabel "Spawner stage context"@en ;
  skos:broader salmon:MeasurementContext ;
  skos:inScheme salmon:MeasurementContextScheme .

salmon:SurvivalOrMortalityContext a skos:Concept ;
  skos:prefLabel "Survival or mortality context"@en ;
  skos:broader salmon:MeasurementContext ;
  skos:inScheme salmon:MeasurementContextScheme .

salmon:LifePhase a skos:Concept ;
  skos:prefLabel "Life phase"@en ;
  skos:inScheme salmon:LifePhaseScheme .

salmon:OceanPhase a skos:Concept ;
  skos:prefLabel "Ocean phase"@en ;
  skos:altLabel "Marine phase"@en ;
  skos:broader salmon:LifePhase ;
  skos:inScheme salmon:LifePhaseScheme .

salmon:TerminalPhase a skos:Concept ;
  skos:prefLabel "Terminal phase"@en ;
  skos:broader salmon:LifePhase ;
  skos:inScheme salmon:LifePhaseScheme .

salmon:MainstemPhase a skos:Concept ;
  skos:prefLabel "Mainstem phase"@en ;
  skos:broader salmon:LifePhase ;
  skos:inScheme salmon:LifePhaseScheme .

salmon:InRiverPhase a skos:Concept ;
  skos:prefLabel "In-river phase"@en ;
  skos:broader salmon:LifePhase ;
  skos:inScheme salmon:LifePhaseScheme .

salmon:SalmonOrigin a skos:Concept ;
  skos:prefLabel "Salmon origin"@en ;
  skos:inScheme salmon:SalmonOriginScheme .

salmon:NaturalOrigin a skos:Concept ;
  skos:prefLabel "Natural-origin"@en ;
  skos:broader salmon:SalmonOrigin ;
  skos:inScheme salmon:SalmonOriginScheme .

salmon:HatcheryOrigin a skos:Concept ;
  skos:prefLabel "Hatchery-origin"@en ;
  skos:broader salmon:SalmonOrigin ;
  skos:inScheme salmon:SalmonOriginScheme .

salmon:LowerBenchmark a skos:Concept ;
  skos:prefLabel "Lower benchmark"@en ;
  skos:definition "Lower threshold benchmark value, often delineating lower status boundaries."@en ;
  skos:inScheme salmon:BenchmarkLevelScheme .

salmon:UpperBenchmark a skos:Concept ;
  skos:prefLabel "Upper benchmark"@en ;
  skos:definition "Upper threshold benchmark value, often delineating higher status boundaries."@en ;
  skos:inScheme salmon:BenchmarkLevelScheme .

#################################################################
# Status vocabularies
#################################################################

salmon:WSPBiologicalStatusZone a skos:Concept ;
  skos:prefLabel "Biological status zone"@en ;
  skos:inScheme salmon:WSPBiologicalStatusZoneScheme .

salmon:GreenZone a skos:Concept ;
  skos:prefLabel "Green zone"@en ;
  skos:broader salmon:WSPBiologicalStatusZone ;
  skos:inScheme salmon:WSPBiologicalStatusZoneScheme .

salmon:AmberZone a skos:Concept ;
  skos:prefLabel "Amber zone"@en ;
  skos:broader salmon:WSPBiologicalStatusZone ;
  skos:inScheme salmon:WSPBiologicalStatusZoneScheme .

salmon:RedZone a skos:Concept ;
  skos:prefLabel "Red zone"@en ;
  skos:broader salmon:WSPBiologicalStatusZone ;
  skos:inScheme salmon:WSPBiologicalStatusZoneScheme .

#################################################################
# Escapement methods and estimate-quality vocabularies
#################################################################

salmon:EnumerationMethod a skos:Concept ;
  skos:prefLabel "Enumeration method"@en ;
  skos:inScheme salmon:EnumerationMethodScheme .

salmon:AerialSurveyCount a skos:Concept ;
  skos:prefLabel "Aerial survey count"@en ;
  skos:broader salmon:EnumerationMethod ;
  skos:inScheme salmon:EnumerationMethodScheme .

salmon:VisualSnorkelCount a skos:Concept ;
  skos:prefLabel "Visual snorkel count"@en ;
  skos:broader salmon:EnumerationMethod ;
  skos:inScheme salmon:EnumerationMethodScheme .

salmon:FixedSiteCensusManual a skos:Concept ;
  skos:prefLabel "Fixed site census (manual)"@en ;
  skos:broader salmon:EnumerationMethod ;
  skos:inScheme salmon:EnumerationMethodScheme .

salmon:EstimateMethod a skos:Concept ;
  skos:prefLabel "Estimate method"@en ;
  skos:inScheme salmon:EstimateMethodScheme .

salmon:AreaUnderTheCurve a skos:Concept ;
  skos:prefLabel "Area under the curve"@en ;
  skos:altLabel "AUC"@en ;
  skos:broader salmon:EstimateMethod ;
  skos:inScheme salmon:EstimateMethodScheme .

salmon:FixedStationTally a skos:Concept ;
  skos:prefLabel "Fixed-station tally"@en ;
  skos:broader salmon:EstimateMethod ;
  skos:inScheme salmon:EstimateMethodScheme .

salmon:PeakCountAnalysis a skos:Concept ;
  skos:prefLabel "Peak count analysis"@en ;
  skos:broader salmon:EstimateMethod ;
  skos:inScheme salmon:EstimateMethodScheme .

salmon:EstimateType a skos:Concept ;
  skos:prefLabel "Estimate type"@en ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:Type1 a skos:Concept ;
  skos:prefLabel "Type 1"@en ;
  skos:notation "Type1"^^xsd:string ;
  skos:broader salmon:EstimateType ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:Type2 a skos:Concept ;
  skos:prefLabel "Type 2"@en ;
  skos:notation "Type2"^^xsd:string ;
  skos:broader salmon:EstimateType ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:Type3 a skos:Concept ;
  skos:prefLabel "Type 3"@en ;
  skos:notation "Type3"^^xsd:string ;
  skos:broader salmon:EstimateType ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:Type4 a skos:Concept ;
  skos:prefLabel "Type 4"@en ;
  skos:notation "Type4"^^xsd:string ;
  skos:broader salmon:EstimateType ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:Type5 a skos:Concept ;
  skos:prefLabel "Type 5"@en ;
  skos:notation "Type5"^^xsd:string ;
  skos:broader salmon:EstimateType ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:Type6 a skos:Concept ;
  skos:prefLabel "Type 6"@en ;
  skos:notation "Type6"^^xsd:string ;
  skos:broader salmon:EstimateType ;
  skos:inScheme salmon:EstimateTypeScheme .

salmon:DowngradeCriteria a skos:Concept ;
  skos:prefLabel "Downgrade criteria"@en ;
  skos:inScheme salmon:DowngradeCriteriaScheme .
